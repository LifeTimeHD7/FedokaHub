				local Handle = Accessory:WaitForChild("Handle")
	
				if Handle:FindFirstChildWhichIsA("Weld", true) then
					Handle:FindFirstChildWhichIsA("Weld", true):Destroy()
					Handle:BreakJoints()
				else
					Handle:BreakJoints()
				end
	
