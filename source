			VirtualBody.Parent = workspace
			VirtualBody.Name = "VirtualBody"
			VirtualBody.Humanoid.WalkSpeed = 8
			VirtualBody.Humanoid.CameraOffset = Vector3.new(0, StudsOffset, 0)
			VirtualBody:SetPrimaryPartCFrame(CharacterCFrame)
	
