				if Motor then
					Motor:Destroy()
				end
	
				return function(CF, Local)
					if Local then
						Attachment0.CFrame = CF
					else
						Attachment0.WorldCFrame = CF
					end
				end;
			end
	
			local function GetExtraTool()
